{{#let
  (if this.tagName
    (element this.tagName)
    (if @element
      @element
      (element 'div')
    )
  ) as |Element|}}
  <Element href={{if this.link this.link.url}} type={{unless this.link 'button' }} {{on "click" this.invoke}}
    data-test-commander ...attributes>
    {{yield}}
  </Element>
{{/let}}