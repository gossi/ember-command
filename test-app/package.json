{
  "name": "test-app",
  "version": "1.0.0",
  "private": true,
  "description": "Commands for Ember",
  "keywords": [
    "command",
    "link",
    "action",
    "cqs"
  ],
  "license": "MIT",
  "author": "Thomas Gossmann",
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "dev:test-app": "ember serve -e test -p 4300",
    "dev:package": "pnpm run --filter='ember-command' start",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently -g 'npm:lint:*:fix'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "_lint:types": "tsc --noEmit",
    "glint": "glint",
    "test": "ember test -tp 0"
  },
  "dependencies": {
    "ember-command": "workspace:*"
  },
  "dependenciesMeta": {
    "ember-command": {
      "injected": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-static-block": "^7.26.0",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^4.0.4",
    "@embroider/test-setup": "^4.0.0",
    "@embroider/webpack": "^4.0.8",
    "@embroider/compat": "^3.7.0",
    "@embroider/macros": "^1.16.9",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "^1.5.0",
    "@glint/template": "^1.5.0",
    "@glint/environment-ember-loose": "^1.5.0",
    "@glint/environment-ember-template-imports": "1.5.0",
    "@gossi/config-eslint": "^0.13.0",
    "@gossi/config-prettier": "^0.9.1",
    "@gossi/config-template-lint": "^0.8.1",
    "@tsconfig/ember": "^3.0.8",
    "@types/qunit": "^2.19.6",
    "@types/sinon": "^10.0.15",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~5.12.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript": "^5.3.0",
    "ember-concurrency": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-element-helper": "^0.8.6",
    "ember-link": "^3.3.0",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^10.1.1",
    "ember-sinon-qunit": "^7.5.0",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-imports": "^3.4.2",
    "ember-template-lint": "^6.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-ember": "^12.3.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.3.3",
    "qunit": "^2.22.0",
    "qunit-dom": "^2.0.0",
    "sinon": "^19.0.2",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1"
