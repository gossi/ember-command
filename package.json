{
  "name": "ember-command",
  "version": "1.0.0",
  "description": "Commands for Ember",
  "keywords": [
    "ember-addon",
    "command",
    "link",
    "action",
    "cqs"
  ],
  "license": "MIT",
  "author": "Thomas Gossmann",
  "repository": {
    "type": "git",
    "url": "git@github.com:gossi/ember-command.git"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "start": "ember serve",
    "release": "release-it",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "ember-auto-import": "^2.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "^5.0.0",
    "ember-element-helper": "^0.6.0",
    "ember-link": "^1.3.1 || ^2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-modules-amd": "^7.20.11",
    "@babel/preset-env": "^7.20.2",
    "@clark/eslint-config-ember": "^2.2.0",
    "@clark/eslint-config-ember-typescript": "^2.2.0",
    "@clark/eslint-config-node": "^2.2.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.5.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@types/ember-qunit": "^5.0.0",
    "@types/ember-resolver": "^5.0.11",
    "@types/ember__application": "^4.0.0",
    "@types/ember__array": "^4.0.1",
    "@types/ember__component": "^4.0.4",
    "@types/ember__controller": "^4.0.0",
    "@types/ember__debug": "^4.0.1",
    "@types/ember__engine": "^4.0.0",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.1",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.4",
    "@types/ember__runloop": "^4.0.0",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.0.0",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.6.1",
    "@types/ember__utils": "^4.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "@types/sinon": "^10.0.11",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^2.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-concurrency": "^2.2.1",
    "ember-concurrency-decorators": "^2.0.3",
    "ember-concurrency-ts": "^0.3.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-sinon-qunit": "^6.0.0",
    "ember-source": "~4.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.3.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.31.0",
    "eslint-plugin-prettier": "^4.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "qunit": "^2.18.0",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.13.1",
    "release-it-lerna-changelog": "^4.0.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0"
  },
  "resolutions": {
    "@glimmer/component": "^1.1",
    "@glimmer/tracking": "^1.1",
    "ember-link": "2.0.1"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.19.1"
  }
}
