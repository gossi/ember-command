{
  "name": "ember-command",
  "version": "1.0.0",
  "description": "Commands for Ember",
  "keywords": [
    "ember-addon",
    "command",
    "link",
    "action",
    "cqs"
  ],
  "license": "MIT",
  "author": "Thomas Gossmann",
  "homepage": "https://github.com/gossi/ember-command",
  "repository": {
    "type": "git",
    "url": "git@github.com:gossi/ember-command.git"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./test-support": {
      "types": "./dist/test-support/index.d.ts",
      "default": "./dist/test-support/index.js"
    },
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.cjs"
  },
  "scripts": {
    "build": "rollup --config",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepack": "rollup --config",
    "release": "release-it",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.8.4",
    "ember-element-helper": "^0.6.1"
  },
  "peerDependencies": {
    "ember-link": "^1.3.1 | ^2.0.0",
    "@glimmer/component": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/runtime": "^7.21.0",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/addon-dev": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@gossi/config-eslint": "^0.3.1",
    "@gossi/config-prettier": "^0.3.1",
    "@gossi/config-template-lint": "^0.3.1",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "ember-link": "^1.3.1",
    "ember-source": "^4.11.0",
    "ember-template-lint": "^5.6.0",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "release-it": "^15.6.0",
    "rollup": "^3.17.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ts": "^3.2.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./components/command-element.js": "./dist/_app_/components/command-element.js",
      "./helpers/command.js": "./dist/_app_/helpers/command.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "extends": "../package.json"
  }
}
