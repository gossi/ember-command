{
  "extends": "@tsconfig/ember/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./"
  },
  "typedocOptions": {
    "entryPoints": [
      "declarations/*.d.ts",
      "declarations/components/**.d.ts"
    ]
  },
  "include": [
    "declarations/*.d.ts",
    "declarations/**/*.d.ts",
    "types/**/*"
  ]
}
